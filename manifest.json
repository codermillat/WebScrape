{
  "manifest_version": 3,
  "name": "Web Text Extractor",
  "version": "1.0",
  "description": "Extract and clean text content from web pages",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://* http://*"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extract Text"
  },
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
} 